<main class="">   
    <div class="row gx-2">
        <div class="col-12">
            {{#with navigation}}
                <div class="card mt-5 mb-5"> 
                    <div class="nav nav-tabs">                                                          
                        {{#each links}}                            
                            <div class="nav-item">
                                <a href="{{href}}" class="nav-link {{#unless href}}disabled{{/unless}}">{{text}}</a>
                            </div>
                        {{/each}}              
                    </div>
                </div> 
            {{/with}}
            <h1 class="mb-5">{{header}}</h1>
        </div>        
        <div class="col-8">
            {{#with contacts_data}}
                <h3 class="mb-4">{{title}}</h3>
                {{#each buttons}}
                    <button class="btn {{#if isPrimary}}btn-warning{{/if}} {{#if isOutline}}btn-outline-warning{{/if}} me-2 mb-3">{{text}}</button>
                {{/each}}
                {{#with form}}
                    <div class="row gx-2">
                        {{#with input}}
                            {{#with surname}}
                                <div class="col-6 mb-3">
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" value="{{placeholder}}">
                                    <div class="form-text text danger">{{caption}}</div>
                                </div>
                            {{/with}}
                            {{#with name}}
                                <div class="col-6 mb-3">
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" value="{{placeholder}}">
                                    <div class="form-text text danger">{{caption}}</div>
                                </div>                                    
                            {{/with}}
                            {{#with phone}}
                                <div class="col-6 mb-3">
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" value="{{placeholder}}">
                                    <div class="form-text text danger">{{caption}}</div>
                                </div> 
                            {{/with}}
                            {{#with email}}
                                <div class="col-6 mb-3">
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" value="{{placeholder}}">
                                    <div class="form-text text danger">{{caption}}</div>
                                </div> 
                            {{/with}}
                        {{/with}}
                    </div>
                    <div class="mb-4">
                        <hr>
                    </div>
                    {{#each switch}}                           
                        <div class="form-label mb-4">                                   
                            <div class="form-check form-switch mb-1 mt-1">                                       
                                <label class="form-check-label">{{text}}</label>
                                <input name="name" type="checkbox" class="form-check-input" {{#if isActive}}checked{{/if}}>
                            </div>
                        </div>                            
                    {{/each}}
                {{/with}}
                <div class="mb-4">
                    <hr>
                </div>
            {{/with}} 
            {{#with delivery}}
                <h3 class="mb-3">{{title}}</h3>
                <div class="card p-2">
                    <div class="row gx-2">
                        {{#each select}}                        
                            <div class="col-6">
                                <label class="form-label">{{label}}</label>
                                <select class="form-select">
                                    <option selected>{{placeholder}}</option>
                                    {{#each options}}
                                    <option value="{{value}}">{{text}}</option>
                                    {{/each}}
                                </select>
                            </div> 
                        {{/each}}
                    </div>
                </div>
                {{#each delivery_type}}
                    <div class="form-check mt-2">
                        <label class="form-check-label">{{type}}</label>
                        <input name="delivery" type="radio" class="form-check-input">
                    </div>                        
                {{/each}}
                <div class="mb-4 mt-4">
                    <hr>
                </div>
                {{#with payment}}
                    <h3 class="mb-3">{{title}}</h3>
                    {{#each list}}
                        <div class="form-check mt-2">
                            <label class="form-check-label">{{type}}</label>
                            <input name="delivery" type="radio" class="form-check-input">
                        </div>
                        <div class="form-text text danger">{{description}}</div>
                    {{/each}}
                {{/with}}
                <div class="mb-4 mt-4">
                    <hr>
                </div>
                {{#with contact}}
                    <h3 class="mb-3">{{title}}</h3>
                    <div class="row gx-2">                    
                        {{#with surname}}
                            <div class="col-6 mb-3">
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" value="{{placeholder}}">
                                <div class="form-text text danger">{{caption}}</div>
                            </div>
                        {{/with}}
                        {{#with name}}
                            <div class="col-6 mb-3">
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" value="{{placeholder}}">
                                <div class="form-text text danger">{{caption}}</div>
                            </div>                                    
                        {{/with}}
                        {{#with phone}}
                            <div class="col-6 mb-3">
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" value="{{placeholder}}">
                                <div class="form-text text danger">{{caption}}</div>
                            </div> 
                        {{/with}}
                        {{#with email}}
                            <div class="col-6 mb-3">
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" value="{{placeholder}}">
                                <div class="form-text text danger">{{caption}}</div>
                            </div> 
                        {{/with}}
                    </div>
                {{/with}}    
                <div class="mb-4">
                    <hr>
                </div>
                {{#with coment}}
                    <label class="form-label">{{label}}</label>
                    <input class="form-control" value="{{placeholder}}">
                {{/with}}                                         
            {{/with}}
            <div class="mb-4 mt-4">
                <hr>
            </div>
            {{#with coupon}}
                <h3 class="mb-3">{{title}}</h3>
                {{#with select}}
                    {{#with availabel_coupon}}
                        <label class="form-label ">{{label}}</label>
                        <select class="form-select form-select-lg ">
                            <option selected>{{placeholder}}</option>
                            {{#each options}}
                                <option value="{{value}}">{{text}}</option>
                            {{/each}}
                        </select>
                    {{/with}}
                    {{#with friend_coupon}}
                        <label class="form-label mt-3">{{label}}</label>
                        <select class="form-select form-select-sm">
                            <option selected>{{placeholder}}</option>
                            {{#each options}}
                                <option value="{{value}}">{{text}}</option>
                            {{/each}}
                        </select>
                    {{/with}}
                {{/with}}
                {{#with add}}
                    {{#with friend}}
                        <label class="form-label ">{{label}}</label>
                        <select class="form-select form-select-lg ">
                            <option selected>{{placeholder}}</option>
                            {{#each options}}
                                <option value="{{value}}">{{text}}</option>
                            {{/each}}
                        </select>
                        <div class="form-text text danger">{{caption}}</div>
                    {{/with}}               
                    {{#with colleague}}
                        <label class="form-label mt-3">{{label}}</label>
                        <select class="form-select form-select-sm">
                            <option selected>{{placeholder}}</option>
                            {{#each options}}
                                <option value="{{value}}">{{text}}</option>
                            {{/each}}
                        </select>
                        <div class="form-text text danger">{{caption}}</div>
                    {{/with}}
                {{/with}}
            {{/with}}
            <div class="mb-4 mt-4">
                <hr>
            </div>
            {{#with quiz}}
                <h3 class="mb-3">{{title}}</h3>
                {{#with preference}}
                    {{title}}
                    {{#each options}}
                        <div class="form-check mt-2">
                            <label class="form-check-label">{{this}}</label>
                            <input name="delivery" type="radio" class="form-check-input">
                        </div>
                    {{/each}}
                {{/with}}                
                <label class="form-label mt-3">{{your_expirience.label}}</label>
                <input class="form-control">
                <div class="form-text text danger"></div>
                {{#with purchase_frequency}}
                    <label class="form-label mt-3">{{label}}</label>
                    <select class="form-select">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                            <option value="{{value}}">{{text}}</option>
                        {{/each}}
                    </select>
                {{/with}}               
                {{#with factors}}
                    <div class="mt-3">
                        {{title}}
                    </div>
                    {{#each options}}
                        <div class="form-check mt-2">
                            <label class="form-check-label">{{this}}</label>
                            <input name="delivery" type="checkbox" class="form-check-input">
                        </div>
                    {{/each}}
                {{/with}}
            {{/with}}
        </div>
        <div class="col-4">
            <div class="card p-2">                    
                {{#each action_block}}
                    <div>
                        <button class="btn {{#if isSuccess}}btn-success{{/if}} 
                        {{#if isDanger}}btn-danger{{/if}}
                        {{#if isInfo}}btn-info{{/if}} mt-1 mb-1">{{text}}</button>
                    </div>
                {{/each}}                   
            </div>
            <div class="card p-2 mt-3">                    
                {{#with total}}
                    <h3>{{title}}</h3>
                    {{#each item_list}}
                        <div>
                            {{name}}: {{price}}
                        </div>
                    {{/each}}
                    <hr class="mb-2 mt-2">
                    {{#each additional_info}}
                        <div>
                            {{text}}: {{value}}
                        </div>
                    {{/each}}
                    <hr class="mb-2 mt-2">
                    {{#with total_price}}
                        <div>
                            {{text}}: {{value}}
                        </div>
                    {{/with}}
                    <hr class="mb-2 mt-2">
                    {{#each button}}
                        <div>
                            <button class="btn {{#if isPrimary}}btn-primary{{/if}} 
                            {{#if isOutline}}btn-outline-primary{{/if}} me-2 mb-2">
                            {{text}}</button>
                        </div>
                    {{/each}}
                {{/with}}                    
            </div>
        </div>
        <div class="col-12">
            <div class="mb-4 mt-4">
                <hr>
            </div>
        </div>
        {{#each footer}}
            <div class="col-2"> 
                <div>
                    {{#with this.[0]}}        
                        <a class=" nav nav-link" href="{{link}}">{{text}}</a> 
                    {{/with}}
                </div>
                
                <div>
                    {{#with this.[1]}}        
                        <a class="nav nav-link" href="{{link}}">{{text}}</a> 
                    {{/with}}
                </div>
                
                <div>
                    {{#with this.[2]}}        
                        <a class="nav nav-link" href="{{link}}">{{text}}</a> 
                    {{/with}}
                </div>
               
                <div>
                {{#with this.[3]}}        
                    <a class="nav nav-link disabled" href="{{link}}">{{text}}</a> 
                {{/with}}
                </div>
            </div>                               
        {{/each}}         
    </div>
</main> 
        