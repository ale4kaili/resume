
<div class="nav-tabs mb-3 pb-2">
    <div class="row align-items-center">
        <div class="col-md-4">                
            <p>{{number}}</p>
            <h5>{{status}}</h5>                                               
        </div>
        <div class="col-md-2">
            {{#with amount}}
                <div>{{title}}:</div>
                <div><h3>{{value}}</h3></div>
            {{/with}}
        </div>
        <div class="col-md-4">
            {{#each images}}
                <img src="{{this}}" class="img-fluid rounded-start" alt="Product photo">
            {{/each}}
        </div>
        <div class="col-md-2 text-center">
            {{#with button}}
                <div>
                    <a class="btn {{#if isPrimary}}btn-primary{{/if}}" href="{{link}}">{{text}}</a>
                </div>
            {{/with}}
        </div>
    </div>
</div>





   